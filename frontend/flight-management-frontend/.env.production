# .env.production
VITE_APP_TITLE=Flight Management System
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production

# API Base URLs (Production)
VITE_API_BASE_URL=https://api.flightmanagement.com
VITE_REFERENCE_MANAGER_URL=https://api.flightmanagement.com
VITE_FLIGHT_SERVICE_URL=https://api.flightmanagement.com
VITE_ARCHIVE_SERVICE_URL=https://api.flightmanagement.com

# WebSocket URLs (Production)
VITE_WS_BASE_URL=wss://api.flightmanagement.com
VITE_WS_FLIGHTS_URL=wss://api.flightmanagement.com/ws/flights

# Feature Flags
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_WEBSOCKETS=true
VITE_ENABLE_CACHE=true
VITE_ENABLE_ERROR_TRACKING=true

# Cache Settings
VITE_CACHE_TTL=600000
VITE_CACHE_MAX_SIZE=200

# Upload Settings
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=.csv,.xlsx,.xls

# Pagination
VITE_DEFAULT_PAGE_SIZE=20
VITE_MAX_PAGE_SIZE=1000